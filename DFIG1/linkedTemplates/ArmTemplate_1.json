{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DFIG1"
		},
		"testtrigger_properties_pipeline2_parameters_id": {
			"type": "int",
			"defaultValue": 3
		},
		"testtrigger_properties_pipeline2_parameters_servername": {
			"type": "string",
			"defaultValue": "ilhan.database.windows.net"
		},
		"testtrigger_properties_pipeline2_parameters_dbname": {
			"type": "string",
			"defaultValue": "ilhan"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/testtrigger')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pipeline2",
							"type": "PipelineReference"
						},
						"parameters": {
							"id": "[parameters('testtrigger_properties_pipeline2_parameters_id')]",
							"servername": "[parameters('testtrigger_properties_pipeline2_parameters_servername')]",
							"dbname": "[parameters('testtrigger_properties_pipeline2_parameters_dbname')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2023-12-08T12:28:00",
						"timeZone": "GMT Standard Time"
					}
				}
			},
			"dependsOn": []
		}
	]
}